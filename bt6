#include <stdio.h>
#include <stdlib.h>

typedef struct node {
    int data;
    struct node* prev;
    struct node* next;
} Node;

Node* taoPhanTu(int giaTri) {
    Node* p = (Node*)malloc(sizeof(Node));
    p->data = giaTri;
    p->prev = NULL;
    p->next = NULL;
    return p;
}

Node* xoaDau(Node* head) {
    if (head == NULL)
        return NULL;
    Node* temp = head;
    head = head->next;
    if (head != NULL)
        head->prev = NULL;
    free(temp);
    return head;
}

void inDanhSach(Node* head) {
    Node* p = head;
    while (p != NULL) {
        printf("%d", p->data);
        if (p->next != NULL)
            printf("<->");
        p = p->next;
    }
    printf("->NULL\n");
}

int main() {
    Node* n1 = taoPhanTu(1);
    Node* n2 = taoPhanTu(2);
    Node* n3 = taoPhanTu(3);
    Node* n4 = taoPhanTu(4);
    Node* n5 = taoPhanTu(5);

    n1->next = n2;
    n2->prev = n1;
    n2->next = n3;
    n3->prev = n2;
    n3->next = n4;
    n4->prev = n3;
    n4->next = n5;
    n5->prev = n4;

    Node* head = n1;

    head = xoaDau(head);

    inDanhSach(head);

    return 0;
}
